<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
android:layout_width="match_parent"
android:layout_height="match_parent"
android:orientation="vertical" >
<DivLayout_Vertical  android:layout_width="fill_parent"  android:layout_height="fill_parent"    android:background="#ffffff"  android:orientation="vertical"  >
<DivLayout_Horizon  android:layout_width="fill_parent"  android:layout_height="wrap_content"    android:background="#ffffff"  android:orientation="horizontal"  >
<TextView   android:id="@+id/backTextView" android:layout_width="wrap_content"  android:layout_height="wrap_content"  android:layout_marginLeft="5dp" android:layout_marginRight="5dp" android:layout_marginTop="5dp" android:layout_marginBottom="5dp" android:text="back"   android:textSize="16sp" android:textColor="#FFFFFF"   android:gravity="center" >
</TextView>
<TextView   android:id="@+id/titleTextView" android:layout_width="wrap_content"  android:layout_height="wrap_content"  android:layout_marginLeft="5dp" android:layout_marginRight="5dp" android:layout_marginTop="5dp" android:layout_marginBottom="5dp" android:text="需求及故障"   android:textSize="16sp" android:textColor="#169BD5"   android:gravity="center" >
</TextView>
<Button   android:id="@+id/queryButton" android:layout_width="wrap_content"  android:layout_height="wrap_content"  android:layout_marginLeft="10dp" android:layout_marginRight="10dp" android:layout_marginTop="5dp" android:layout_marginBottom="5dp" android:text="查询"   android:textSize="16sp" android:textColor="#989898"   android:background="背景颜色"  >
</Button>
</DivLayout_Horizon>
<DivLayout_Vertical  android:layout_width="fill_parent"  android:layout_height="wrap_content"    android:background="#FFFFFF"   android:orientation="vertical"  android:gravity="">
<DivLayout_Horizon  android:layout_width="fill_parent"  android:layout_height="wrap_content"    android:background="#ffffff"  android:orientation="horizontal"  >
<EditText   android:id="@+id/dealNameEditText" android:layout_width="wrap_content"  android:layout_height="wrap_content"  android:layout_marginLeft="5dp" android:layout_marginRight="5dp" android:layout_marginTop="5dp" android:layout_marginBottom="5dp" android:hint="处理人"   android:textSize="16sp" android:textColor="#858585"   android:gravity="center"   android:singleLine="true"   android:maxLength="40"  >
</EditText>
</DivLayout_Horizon>
<DivLayout_Horizon  android:layout_width="fill_parent"  android:layout_height="wrap_content"    android:background="#ffffff"  android:orientation="horizontal"  >
<TextView   android:id="@+id/zhiTextView" android:layout_width="wrap_content"  android:layout_height="wrap_content"  android:layout_marginLeft="5dp" android:layout_marginRight="5dp" android:layout_marginTop="5dp" android:layout_marginBottom="5dp" android:text="至"   android:textSize="16sp" android:textColor="#636363"   android:gravity="center" >
</TextView>
</DivLayout_Horizon>
<DivLayout_Horizon  android:layout_width="fill_parent"  android:layout_height="wrap_content"    android:background="#ffffff"  android:orientation="horizontal"  >
<Button   android:id="@+id/summitButton" android:layout_width="wrap_content"  android:layout_height="wrap_content"  android:layout_marginLeft="10dp" android:layout_marginRight="10dp" android:layout_marginTop="5dp" android:layout_marginBottom="5dp" android:text="提交"   android:textSize="16sp" android:textColor="#FFFFFF"   android:background="#169BD5"  >
</Button>
</DivLayout_Horizon>
</DivLayout_Vertical>
<ListLayout  android:layout_width="fill_parent"  android:layout_height="wrap_content"    android:background="#FFFFFF"   android:orientation="horizontal"  android:gravity="">
<List_ItemLayout_Vertical  android:layout_width="fill_parent"  android:layout_height="wrap_content"    android:background="#FFFFFF"   android:orientation="vertical"  android:gravity="">
<DivLayout_Horizon  android:layout_width="fill_parent"  android:layout_height="wrap_content"    android:background="#ffffff"  android:orientation="horizontal"  >
<TextView   android:id="@+id/modelNameTextView" android:layout_width="wrap_content"  android:layout_height="wrap_content"  android:layout_marginLeft="5dp" android:layout_marginRight="5dp" android:layout_marginTop="5dp" android:layout_marginBottom="5dp" android:text="模块名称"   android:textSize="16sp" android:textColor="#636363"   android:gravity="center" >
</TextView>
</DivLayout_Horizon>
<DivLayout_Horizon  android:layout_width="fill_parent"  android:layout_height="wrap_content"    android:background="#FFFFFF"   android:orientation="horizontal"  android:gravity="">
<TextView   android:id="@+id/applicationNameTextView" android:layout_width="wrap_content"  android:layout_height="wrap_content"  android:layout_marginLeft="5dp" android:layout_marginRight="5dp" android:layout_marginTop="5dp" android:layout_marginBottom="5dp" android:text="系统名称"   android:textSize="16sp" android:textColor="#636363"   android:gravity="center" >
</TextView>
<TextView   android:id="@+id/lineTextView" android:layout_width="wrap_content"  android:layout_height="wrap_content"  android:layout_marginLeft="5dp" android:layout_marginRight="5dp" android:layout_marginTop="5dp" android:layout_marginBottom="5dp" android:text="|"   android:textSize="16sp" android:textColor="#636363"   android:gravity="center" >
</TextView>
<TextView   android:id="@+id/questionTypeTextView" android:layout_width="wrap_content"  android:layout_height="wrap_content"  android:layout_marginLeft="5dp" android:layout_marginRight="5dp" android:layout_marginTop="5dp" android:layout_marginBottom="5dp" android:text="需求及建议"   android:textSize="16sp" android:textColor="#636363"   android:gravity="center" >
</TextView>
<TextView   android:id="@+id/feedBackDateTimeTextView" android:layout_width="wrap_content"  android:layout_height="wrap_content"  android:layout_marginLeft="5dp" android:layout_marginRight="5dp" android:layout_marginTop="5dp" android:layout_marginBottom="5dp" android:text="2018"   android:textSize="16sp" android:textColor="#636363"   android:gravity="center" >
</TextView>
</DivLayout_Horizon>
<DivLayout_Horizon  android:layout_width="fill_parent"  android:layout_height="wrap_content"    android:background="#ffffff"  android:orientation="horizontal"  >
<TextView   android:id="@+id/questionTextView" android:layout_width="fill_parent"  android:layout_height="wrap_content"  android:layout_marginLeft="5dp" android:layout_marginRight="5dp" android:layout_marginTop="5dp" android:layout_marginBottom="5dp" android:text="sfds"   android:textSize="16sp" android:textColor="#636363"   android:gravity="center" >
</TextView>
</DivLayout_Horizon>
</List_ItemLayout_Vertical>
</ListLayout>
</DivLayout_Vertical>
</LinearLayout>




