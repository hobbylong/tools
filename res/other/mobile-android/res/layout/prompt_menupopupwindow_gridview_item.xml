<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_width="fill_parent"
  android:layout_height="fill_parent">
  	<RelativeLayout
  		android:id = "@+id/rlayout_item_gridview"
  		android:layout_width = "wrap_content"
  		android:layout_height = "wrap_content"
  		android:layout_marginLeft="1.33dp"
  		android:layout_centerInParent = "true"
  		android:paddingBottom="8dp">
  		<ImageView
  			android:id = "@+id/img_gridview_item"
  			android:layout_width = "wrap_content"
  			android:layout_height = "wrap_content"
  			android:layout_marginTop="6.67dp"
  			android:layout_centerHorizontal = "true"
  			/>
  		<TextView
  			android:id = "@+id/tv_gridview_item"
  			android:layout_width = "wrap_content"
  			android:layout_height = "wrap_content"
  			android:layout_below = "@id/img_gridview_item"
  			android:textColor="@android:color/black"
  			android:layout_centerHorizontal = "true"
  			android:layout_marginBottom="6.67px"
  			/>
  	</RelativeLayout>
</RelativeLayout>
